#!/usr/bin/env bash

_cdd() {
   local dirnames
   cur="${COMP_WORDS[COMP_CWORD]}"
   prev="${COMP_WORDS[COMP_CWORD-1]}"

   if [ ${#COMP_WORDS[*]} -lt 3 ]; then
      dirnames="$(pwd | xargs dirname | tr '/' ' ')"

      COMPREPLY=( $(compgen -W "${dirnames}" -- ${cur}) )
   fi
} &&
complete -F _cdd cdd

